        "/api/players": {
            "get": {
                "tags": [
                    "Players"
                ],
                "summary": "Retrieve Players",
                "description": "Retrieves a list of all unique player names.",
                "operationId": "api_players_api_players_get",
                "responses": {
                    "200": {
                        "description": "A list of unique player names",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PlayerList"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Players"
                ],
                "summary": "Add a New Player",
                "description": "Adds a new player name to the list.\n\n- **name**: The name of the player to add. It is taken from the form data.\n\nThis endpoint writes the new player's name to the players file, appending it to the end.\nIf the operation is successful, it returns a success message. Otherwise, it raises an HTTPException.",
                "operationId": "api_players_add_api_players_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PlayerCreation"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Confirmation of player addition",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PlayerList"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/players/": {
            "delete": {
                "tags": [
                    "Players"
                ],
                "summary": "Delete a Player",
                "description": "Deletes a player name from the list.\n\n- **name**: The name of the player to delete.\n\nThis endpoint reads all player names, filters out the specified name, and rewrites the file without it.\nIf the operation is successful, it returns a success message.",
                "operationId": "delete_name_api_players__delete",
                "parameters": [
                    {
                        "name": "name",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "description": "The name of the player to delete.",
                            "title": "Name"
                        },
                        "description": "The name of the player to delete."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Confirmation of player deletion",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/BasicResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/players/submit": {
            "post": {
                "tags": [
                    "Players"
                ],
                "summary": "Submit Player Names",
                "description": "Submits a list of player names.\n\n- **names**: A list of names to be submitted.\n\nAccepts a list of names in the request body and submits them.",
                "operationId": "api_players_submit_api_players_submit_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PlayerList"
                            }
