                    },
                    "downloaded": {
                        "type": "boolean",
                        "title": "Downloaded"
                    },
                    "edition": {
                        "type": "string",
                        "title": "Edition"
                    },
                    "golden": {
                        "type": "boolean",
                        "title": "Golden"
                    },
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "language": {
                        "type": "string",
                        "title": "Language"
                    },
                    "rating": {
                        "type": "number",
                        "title": "Rating"
                    },
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "views": {
                        "type": "integer",
                        "title": "Views"
                    }
                },
                "type": "object",
                "required": [
                    "artist",
                    "downloaded",
                    "edition",
                    "golden",
                    "id",
                    "language",
                    "rating",
                    "title",
                    "views"
                ],
                "title": "Song"
            },
            "SongsResponse": {
                "properties": {
                    "paging": {
                        "$ref": "#/components/schemas/Paging"
                    },
                    "songs": {
                        "items": {
                            "$ref": "#/components/schemas/Song"
                        },
                        "type": "array",
                        "title": "Songs"
                    }
                },
                "type": "object",
                "required": [
                    "paging",
                    "songs"
                ],
                "title": "SongsResponse"
            },
            "UdEnum": {
                "type": "string",
                "enum": [
                    "asc",
                    "desc"
