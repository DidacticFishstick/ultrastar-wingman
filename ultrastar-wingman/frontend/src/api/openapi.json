        "/api/usdb/songs": {
            "get": {
                "tags": [
                    "USDB"
                ],
                "summary": "Search Songs",
                "operationId": "api_usdb_songs_api_usdb_songs_get",
                "parameters": [
                    {
                        "name": "artist",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter songs by the artist's name.",
                            "title": "Artist"
                        },
                        "description": "Filter songs by the artist's name."
                    },
                    {
                        "name": "title",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter songs by title.",
                            "title": "Title"
                        },
                        "description": "Filter songs by title."
                    },
                    {
                        "name": "edition",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter by the song's edition.",
                            "title": "Edition"
                        },
                        "description": "Filter by the song's edition."
                    },
                    {
                        "name": "language",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter songs by language.",
                            "title": "Language"
                        },
                        "description": "Filter songs by language."
                    },
                    {
                        "name": "genre",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter songs by genre.",
                            "title": "Genre"
                        },
                        "description": "Filter songs by genre."
                    },
                    {
                        "name": "order",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/OrderEnum"
                                }
                            ],
                            "description": "Sort the result by this order criteria.",
                            "default": "rating",
                            "title": "Order"
                        },
                        "description": "Sort the result by this order criteria."
                    },
                    {
                        "name": "ud",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/UdEnum"
                                }
                            ],
                            "description": "Sort order: ascending (asc) or descending (desc).",
                            "default": "desc",
                            "title": "Ud"
                        },
                        "description": "Sort order: ascending (asc) or descending (desc)."
                    },
                    {
                        "name": "golden",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": false,
                            "title": "Golden"
                        }
                    },
                    {
                        "name": "songcheck",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": false,
                            "title": "Songcheck"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "description": "The number of songs to return per page.",
                            "default": 30,
                            "title": "Limit"
                        },
                        "description": "The number of songs to return per page."
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "description": "Page number for pagination.",
                            "default": 1,
                            "title": "Page"
                        },
                        "description": "Page number for pagination."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A list of songs matching the criteria",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SongsResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
